<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.theinventoryapp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_logo_custom"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_logo_custom_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".TransactionHistoryActivity" />
        <activity android:name=".GiveItemsActivity" />
        <activity android:name=".AddItemsAcitivity" />
        <activity android:name=".ValuablesInventoryActivity" />
        <activity android:name=".ConsumablesInventoryActivity" />
        <activity android:name=".DatabseActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>


</manifest>